    <Topic Key="{BC4352CA-9F4F-460F-9014-13FF0E4F2775}" Name="The_Cartagena_Protocol_on_Biosafety" Title="The Cartagena Protocol on Biosafety" Titlem="29-02-2008 09:17:14" ContentsTitle="" ContentsTitlem="29-02-2008 09:17:24" Status="In Progress" CreatedOn="29-02-2008 09:16:47" ModifiedOn="04-09-2008 11:38:39" LayoutName="" AllStyleSheets="True" AllScripts="True" AutoGenerateIndexEntry="True" IncludeInHelpFile="True" HelpContextIDs="" Category="{47F83A99-A03C-410E-9C2D-47AE1253911D}">
