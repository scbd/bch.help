    <Topic Key="{1CAAB988-36EF-48B0-9F59-A7A516534C72}" Name="How_does_the_BCH_work" Title="How does the BCH work?" Titlem="25-02-2008 10:36:50" ContentsTitle="" ContentsTitlem="25-02-2008 10:57:56" Status="In Progress" CreatedOn="25-02-2008 10:36:50" ModifiedOn="13-08-2008 14:54:34" LayoutName="" AllStyleSheets="True" AllScripts="True" AutoGenerateIndexEntry="True" IncludeInHelpFile="True" HelpContextIDs="" Category="{D6182699-C124-44D8-9248-2E04AEBD5E8E}">
        <Notes/>
